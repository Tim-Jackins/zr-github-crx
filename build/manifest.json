{
  "manifest_version": 2,
  "name": "ZR Github",
  "description": "This extension adds Github capabilities to the ZR website",
  "version": "1.0",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "Github interface",
    "default_popup": "popup.html"
  },
   "permissions": [
    "activeTab",
    "identity",
    "storage",
    "tabs",
    "http://www.zerorobotics.mit.edu/",
    "https://github.com/"
  ]
}
